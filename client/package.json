  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@vitejs/plugin-react": "^4.4.0",
    "concurrently": "^9.1.2",
    "vite": "^6.3.1"
