  "scripts": {
    "dev": "VITE_CJS_IGNORE_WARNING=true vite --host --port 3000 --logLevel warn",
    "start": "VITE_CJS_IGNORE_WARNING=true vite --host --port 3000 --logLevel warn",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "start:all": "concurrently \"cd ../server && uvicorn main:app --reload\" \"npm run dev\"",
